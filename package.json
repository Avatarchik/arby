        "cors": "^2.8.4",
        "country-list": "^2.1.0",
        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "express-session": "^1.15.6",
        "got": "^9.2.2",
        "jsonschema": "^1.2.4",
        "leven": "^2.1.0",
        "lodash": "^4.17.11",
        "moment": "^2.22.2",
